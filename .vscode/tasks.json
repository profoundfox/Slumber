{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build Debug",
      "type": "process",
      "command": "dotnet",
      "args": [
        "build",
        "--configuration",
        "Debug"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": "$msCompile"
    },
    {
      "label": "build Release",
      "type": "process",
      "command": "dotnet",
      "args": [
        "build",
        "--configuration",
        "Release"
      ],
      "group": "build",
      "problemMatcher": "$msCompile"
    }
  ]
}
